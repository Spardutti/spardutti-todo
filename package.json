{
  "name": "spardutti-todo",
  "productName": "spardutti-todo",
  "version": "1.1.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start -- --no-sandbox",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest",
    "build:analyze": "vite build --mode analyze",
    "dist:win": "electron-forge package --platform win32 --arch x64 && electron-builder --win --x64 --prepackaged out/spardutti-todo-win32-x64 --publish never",
    "dist:linux": "electron-forge package --platform linux --arch x64 && electron-builder --linux --x64 --prepackaged out/spardutti-todo-linux-x64 --publish never"
  },
  "keywords": [],
  "author": {
    "name": "Spardutti",
    "email": "luisdamian.sp@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spardutti/spardutti-todo.git"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@reforged/maker-appimage": "^5.1.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "39.2.3",
    "electron-builder": "^26.0.12",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^27.2.0",
    "typescript": "~5.9.2",
    "vite": "^5.4.21",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "@toon-format/toon": "1.0.0",
    "electron-log": "5.4.1",
    "electron-updater": "6.7.1",
    "rollup-plugin-visualizer": "6.0.5"
  }
}
